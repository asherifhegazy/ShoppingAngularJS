<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="redirect urls to index.html">
                    <match url=".*\html|css/|img/|lib/|favicon.ico|app/|assets/|shared/|components/|node_modules/" negate="true" />
                    <action type="Rewrite" url="/index.html" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
